{
  "name": "quickstart",
  "version": "0.0.9",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "prod": "set NODE_ENV=production & node ./bin/www",
    "debug": "set DEBUG=quickstart:* & nodemon ./bin/www",
    "dev": "nodemon ./bin/www",
    "prettier": "prettier --write --tab-width 4 --single-quote **/*.{ts,js,css,html}",
    "test": "mocha ./test",
    "jsx": "npx babel --watch src/public/javascripts/storage/react/src --out-dir src/public/javascripts/storage/react --presets react-app\\prod"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "src/public/*",
      "uploads/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "debug": "^2.6.9",
    "ejs": "^2.7.4",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "finale-rest": "^1.1.1",
    "fs-extra": "^9.0.1",
    "http-errors": "^1.7.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-pre-gyp": "^0.14.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "sequelize": "^5.22.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "husky": "^4.2.5",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "repository": "https://github.com/WeiLiangLOL/quickstart",
  "keywords": [
    "quick",
    "start",
    "data",
    "manager"
  ],
  "contributors": [
    {
      "name": "Jie Xiang",
      "url": "https://github.com/akgrenSoar"
    },
    {
      "name": "Wei Liang",
      "url": "https://github.com/WeiLiangLOL"
    }
  ],
  "license": "GNU GPLv3"
}
